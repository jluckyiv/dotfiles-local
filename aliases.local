#! /bin/sh

if type nvim > /dev/null 2>&1; then
  alias vim='nvim'
  alias vi='nvim'
else
  alias vi='vim'
fi

alias an='ansible'
alias anp='ansible-playbook'
alias bw='bower'
alias d='docker'
alias dc='docker-compose'
alias dm='docker-machine'
alias e='elm'
alias em='ember'
alias ex='elixir'
alias git='hub'
alias r='rails'
alias rb='ruby'
alias so='source'
